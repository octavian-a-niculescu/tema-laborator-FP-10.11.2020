#include <iostream>

using namespace std;

void citire(int& n, int*& a, int &k)
{
    cin >> n;
    a = new int[100];
    for (int i = 0; i < n; i++)
    {
        cin >> *(a + i);
    }
    cin >> k;
}

void afisare(int n, int* a)
{
    for (int i = 0; i < n; i++)
    {
        cout << *(a+i) << " ";
    }
}

int suma(int i, int j, int* a)
{
    return *(a + i) + *(a + j);
}

int produs(int i, int j, int* a)
{
    return *(a + i) * *(a + j);
}

void permutareLaStanga(int n, int*& a, int p)
{
    int nrOperatii = n - 2*p;
    while (nrOperatii != 0)
    {
        int aux = *(a + p);
        for (int i = p; i < n - 1; i++)
        {
            *(a + i) = *(a + i + 1);
        }
        *(a + n - 1) = aux;
        nrOperatii--;
    }
}

void inlocuire(int n, int*& a, int p)
{
    int i = 0;
    for (int k = n-p; k < n; k++)
    {
        int aux = *(a + k);
        *(a + k) = *(a + i);
        *(a + i) = aux;
        i++;
    }
}

int main()
{
    int n, * a, k;
    citire(n, a, k);
    inlocuire(n, a, k);
    permutareLaStanga(n, a, k);
    afisare(n, a);
    return 0;
}
